{
  "vimMode": true,
  "authAccept": true,
  "showMemoryUsage": true,
  "showLineNumbers": true,
  "hasSeenIdeIntegrationNudge": true,
  "selectedAuthType": "qwen-oauth",
  "tools": {
    "approvalMode": "yolo"
  },
  "mcpServers": {
    "kai-notify": {
      "command": "npx",
      "args": [
        "kai-notify",
        "--mcp"
      ]
    },
    "shadcn": {
      "command": "npx",
      "args": [
        "-y",
        "shadcn@canary",
        "registry:mcp"
      ],
      "env": {
        "REGISTRY_URL": "https://tweakcn.com/r/themes/registry.json"
      }
    }
  }
}
